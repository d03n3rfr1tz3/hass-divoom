{
  "domain": "divoom",
  "name": "Divoom",
  "bluetooth": [
    {
      "local_name": "Ditoo*"
    },
    {
      "local_name": "Pixoo*"
    },
    {
      "local_name": "Timebox*"
    },
    {
      "local_name": "Tivoo*"
    }
  ],
  "codeowners": ["@d03n3rfr1tz3"],
  "config_flow": true,
  "dependencies": [ "bluetooth_adapters" ],
  "documentation": "https://github.com/d03n3rfr1tz3/hass-divoom/blob/master/README.md",
  "iot_class": "local_push",
  "issue_tracker": "https://github.com/d03n3rfr1tz3/hass-divoom/issues",
  "loggers": ["divoom"],
  "requirements": [ "pillow>=7.2.0" ],
  "version": "1.1.0",
  "zeroconf": [
    {
      "type": "_divoom_esp32._tcp.local.",
      "properties": {
        "device_name": "ditoo*"
      }
    },
    {
      "type": "_divoom_esp32._tcp.local.",
      "properties": {
        "device_name": "pixoo*"
      }
    },
    {
      "type": "_divoom_esp32._tcp.local.",
      "properties": {
        "device_name": "timebox*"
      }
    },
    {
      "type": "_divoom_esp32._tcp.local.",
      "properties": {
        "device_name": "tivoo*"
      }
    }
  ]
}
